class TutorialManager : ZilchComponent
{

    
    function Initialize(init : CogInitializer)
    {
        var sequence = Actions.Sequence(this.Owner.Actions);
        Actions.Delay(sequence,10);
        Actions.Property(
            sequence,
            @this.Owner.Sprite.Color,
            Real4 (1,1,1,1),
            2, Ease.QuadInOut);
    }
}
